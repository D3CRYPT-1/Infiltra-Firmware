{
  "build": {
    "core": "esp32",
    "mcu": "esp32",
    "f_cpu": "240000000L",
    "variant": "m5stick-c",
    "partitions": "huge_app.csv",
    "flash_mode": "qio",
    "f_flash": "40000000L",
    "filesystem": "spiffs"
  },
  "upload": {
    "protocol": "esptool",
    "speed": 115200
  },
  "frameworks": ["arduino"],
  "name": "M5Stick-C Plus 1.1",
  "vendor": "M5Stack",
  "monitor": {
    "speed": 115200
  },
  "build_flags": [
    "-fno-lto",
    "-Os",
    "-ffunction-sections",
    "-fdata-sections",
    "-Wl,--gc-sections",
    "-DCORE_DEBUG_LEVEL=0",
    "-DM5STICK_C_PLUS_1_1",
    "-DBTN_A_PIN=37",
    "-DBTN_B_PIN=39",
    "-DBTN_C_PIN=0",
    "-DUSER_SETUP_LOADED=1",
    "-DST7789_2_DRIVER=1",
    "-DTFT_RGB_ORDER=1",
    "-DTFT_WIDTH=135",
    "-DTFT_HEIGHT=240",
    "-DTFT_BACKLIGHT_ON=0",
    "-DTFT_CS=5",
    "-DTFT_DC=23",
    "-DTFT_RST=18",
    "-DTOUCH_CS=-1",
    "-DTFT_MOSI=15",
    "-DTFT_SCLK=13",
    "-DTFT_BL=-1",
    "-DSMOOTH_FONT=1",
    "-DSPI_FREQUENCY=20000000",
    "-DSPI_READ_FREQUENCY=20000000",
    "-DSPI_TOUCH_FREQUENCY=2500000"
  ],
  "upload_flags": [
    "--before", "default_reset",
    "--after", "hard_reset",
    "--no-stub"
  ]
}
